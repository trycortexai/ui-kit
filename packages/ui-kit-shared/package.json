{
	"name": "@cortex-ai/ui-kit-shared",
	"description": "Shared types and helpers for Cortex UI components",
	"version": "0.2.10",
	"type": "module",
	"files": [
		"dist"
	],
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"./chat": {
			"import": {
				"types": "./dist/chat.d.ts",
				"default": "./dist/chat.js"
			}
		},
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./cortex": {
			"import": {
				"types": "./dist/cortex.d.ts",
				"default": "./dist/cortex.js"
			}
		},
		"./common": {
			"import": {
				"types": "./dist/common.d.ts",
				"default": "./dist/common.js"
			}
		},
		"./bridge/client": {
			"import": {
				"types": "./dist/bridge/client.d.ts",
				"default": "./dist/bridge/client.js"
			}
		},
		"./bridge": {
			"import": {
				"types": "./dist/bridge/index.d.ts",
				"default": "./dist/bridge/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"license": "MIT",
	"scripts": {
		"type-check": "tsc --noEmit"
	},
	"peerDependencies": {
		"typescript": ">=4.5.0"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		}
	},
	"dependencies": {
		"@cortex-ai/sdk": "^0.7.4"
	}
}
